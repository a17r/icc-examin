
exe=iccexamin
path="${0%/*}"
export RESOURCESPATH="${path%/*}/Resources$prefix"
binpath="$RESOURCESPATH/bin"
test -z $DYLD_FALLBACK_LIBRARY_PATH && \
export DYLD_FALLBACK_LIBRARY_PATH="$RESOURCESPATH/lib:$RESOURCESPATH/lib/elektra" ||
export DYLD_FALLBACK_LIBRARY_PATH="$RESOURCESPATH/lib:$RESOURCESPATH/lib/elektra":$DYLD_FALLBACK_LIBRARY_PATH
export PATH=$binpath:$PATH:$prefix/bin

test -z $XDG_DATA_DIRS && \
XDG_DATA_DIRS="$RESOURCESPATH/share" ||
XDG_DATA_DIRS="$RESOURCESPATH/share:$XDG_DATA_DIRS"

test -z $OY_MODULE_PATHS && \
OY_MODULE_PATHS="$RESOURCESPATH/lib/oyranos:$RESOURCESPATH/lib/colour/modules" ||
OY_MODULE_PATHS="$RESOURCESPATH/lib/oyranos:$RESOURCESPATH/lib/colour/modules:$OY_MODULE_PATHS"

OY_LOCALEDIR="$RESOURCESPATH/locale"

export XDG_DATA_DIRS OY_MODULE_PATHS OY_LOCALEDIR



#set >> /tmp/test.txt
"$path/$exe" $@
